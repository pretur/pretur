{
  "name": "pretur.autogen",
  "version": "0.2.0",
  "description": "Handlebars based code generation helper",
  "main": "lib/main.js",
  "typings": "lib/main.d.ts",
  "files": [
    "lib",
    "typings.json"
  ],
  "scripts": {
    "build": "tsc -d ../../typings/index.d.ts src/main.ts --rootDir src --outDir lib --module commonjs --removeComments",
    "prepublish": "npm run build"
  },
  "keywords": [
    "pretur",
    "template",
    "handlebars",
    "code",
    "auto",
    "generation"
  ],
  "author": "Ali Taheri Moghaddar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "bugs": {
    "url": "https://github.com/pretur/pretur/issues"
  },
  "homepage": "https://github.com/pretur/pretur/tree/master/packages/pretur.autogen",
  "dependencies": {
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "inflection": "^1.10.0",
    "lodash": "^4.15.0",
    "watchr": "^2.6.0"
  },
  "devDependencies": {
    "typescript": "^2.1.0-dev.20160823"
  }
}
