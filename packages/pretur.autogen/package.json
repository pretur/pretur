  },
  "homepage": "https://github.com/pretur/pretur/tree/master/packages/pretur.autogen",
  "dependencies": {
    "@types/fs-extra": "0.0.32",
    "@types/handlebars": "^4.0.31",
    "@types/inflection": "^1.5.28",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.41",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "inflection": "^1.10.0",
