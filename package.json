{
  "name": "pretur",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "for": "node runFor.js",
    "for-all": "node runFor.js --all",
    "prepublish-all": "npm test",
    "publish-all": "node runFor.js --all npm -- publish",
    "bump": "lerna publish --skip-git",
    "lint": "tslint -e \"packages/*/node_modules/**\" -e \"packages/*/lib/**\" \"packages/**/*.ts\" \"packages/**/*.tsx\"",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register \"packages/**/*.spec.ts\"",
    "postinstall": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.55",
    "@types/react-addons-test-utils": "^0.14.16",
    "chai": "^3.5.0",
    "lerna": "2.0.0-beta.31",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "ts-node": "^2.0.0",
    "tslint": "^4.2.0",
    "tslint-eslint-rules": "^3.2.3",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.2.0-dev.20161231"
  }
}
