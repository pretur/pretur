{
  "name": "pretur",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lerna:publish": "lerna publish --skip-npm --skip-git",
    "lerna:bootstrap": "lerna exec -- npm install",
    "test": "mocha --compilers ts:ts-node/register --compilers tsx:ts-node/register packages/**/*.spec.ts",
    "postinstall": "typings install && npm run lerna:bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "lerna": "2.0.0-beta.26",
    "mocha": "^3.0.2",
    "ts-node": "^1.2.2",
    "typescript": "^2.1.0-dev.20160814",
    "typings": "^1.3.2"
  }
}