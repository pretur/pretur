{
  "name": "pretur",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lerna": "lerna",
    "lerna:bootstrap": "lerna exec -- npm install",
    "lerna:publish": "lerna publish --skip-npm --skip-git",
    "publish-all": "lerna exec -- npm publish",
    "lint": "tslint -e \"packages/*/node_modules/**\" -e \"packages/*/lib/**\" \"packages/**/*.ts\" \"packages/**/*.tsx\"",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register \"packages/**/*.spec.ts\"",
    "postinstall": "typings install && npm run lerna:bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/mocha": "^2.2.31",
    "@types/react-addons-test-utils": "^0.14.14",
    "chai": "^3.5.0",
    "lerna": "2.0.0-beta.26",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^1.3.0",
    "tslint-microsoft-contrib": "^2.0.10",
    "typescript": "^2.1.0-dev.20160828",
    "typings": "^1.3.3"
  }
}
