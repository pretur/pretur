{
  "name": "pretur",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "for": "node runFor.js",
    "for-all": "node runFor.js --all",
    "prepublish-all": "npm test",
    "publish-all": "node runFor.js --all npm -- publish",
    "bump": "lerna publish --skip-git",
    "lint": "tslint -e \"packages/*/node_modules/**\" -e \"packages/*/lib/**\" \"packages/**/*.ts\" \"packages/**/*.tsx\"",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register \"packages/**/*.spec.ts\"",
    "postinstall": "npm run for-all -- npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.47",
    "@types/react-addons-test-utils": "^0.14.15",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "ts-node": "^1.7.0",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^2.1.0",
    "tslint-microsoft-contrib": "^2.0.13",
    "typescript": "next",
    "lerna": "2.0.0-beta.26"
  }
}
