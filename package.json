{
  "name": "pretur",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "for": "node runFor.js",
    "for-all": "node runFor.js --all",
    "install-all": "npm run for-all npm -- install",
    "build-all": "npm run for-all npm -- run build",
    "bump": "lerna publish --skip-git",
    "lerna": "lerna",
    "lint": "tslint -e \"packages/*/node_modules/**\" -e \"packages/*/lib/**\" \"packages/**/*.ts\" \"packages/**/*.tsx\"",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register \"packages/**/*.spec.ts\"",
    "bootstrap": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretur/pretur"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.9",
    "@types/react-addons-test-utils": "^0.14.19",
    "chai": "^4.0.2",
    "lerna": "^2.0.0",
    "mocha": "^3.4.2",
    "react-addons-test-utils": "^15.6.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "2.4.0"
  }
}
